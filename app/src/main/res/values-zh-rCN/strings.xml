    <string name="setting_title_font">字体大小</string>
    <string name="setting_summary_font">改变网页的字体大小</string>
    <string name="setting_title_remote">远程内容</string>
    <string name="setting_summary_remote">启用远程内容，远程内容不是直接来自于你访问的网站。
        禁用这个特性使访问更加安全快捷同时节省数据存储。但是页面中有些内容将不再被展示。</string>
    <string name="setting_title_save_data">请求 \'存储数据\'</string>
    <string name="setting_summary_save_data">告诉网站你倾向于储存数据。下次可能会载入得比较快</string>
    <!-- browser start -->
    <string name="setting_title_images">图片</string>
    <string name="setting_summary_images">支持网页图片。如果关闭，可以节省网络流量，网页载入也会比较快</string>
    <string name="setting_title_cookie">Cookies</string>
    <string name="setting_summary_cookie_delete">打开它会清除所有网站的Cookies（包含被信任的网站）.</string>
    <string name="setting_summary_cookie">开启 Cookies. Cookies 是存在你设备上小的文本文件。网站用他们来完成一些功能，例如存储用户设置（例如搜索设置）。
        但是他们也可能诶用来搜集用户个人信息。第三方的 Cookies（搜集用户信息为广告服务）总是被禁用的。</string>
    <string name="setting_title_whitelistCookie">Cookie 白名单</string>
    <string name="setting_summary_whitelistCookie">如果你禁用 Cookies，可以使用白名单来允许你信任的网站使用保存的 cookies.</string>
    <string name="setting_title_javascript">JavaScript 设置</string>
    <string name="setting_summary_javascript">启用 JavaScript. 许多网站需要 JavaScript 来达成一些功能(菜单,
        载入以及显示图片, …). 但启用 Javascript 可能会引入一些潜在风险</string>
    <string name="setting_title_whitelistJS">JavaScript 白名单</string>
    <string name="setting_summary_whitelistJS">如果你禁用 JavaScript，可以使用白名单来允许你信任的网站使用 javascript.</string>
    <string name="setting_title_adblock">AdBlock</string>
    <string name="setting_summary_adblock">开启 AdBlock 以过滤烦人的广告，同时页面加载也会更快。</string>
    <string name="setting_title_whitelist">AdBlock 白名单</string>
    <string name="setting_summary_whitelist">允许部分页面显示广告。有些页面在过滤了广告后会出现异常。</string>
    <string name="setting_title_location">位置</string>
    <string name="setting_summary_location">允许网站访问你的位置信息。有些网站（如何谷歌导航）需要这个功能。</string>

    <string name="setting_title_history">历史</string>
    <string name="setting_summary_history">保存浏览历史记录。</string>


    <!-- browser clear -->
